<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/foodEntryPage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#79CDF3"
    tools:context=".foodEntryPage">

    <TextView
        android:id="@+id/date"
        android:layout_width="2000dp"
        android:layout_height="51dp"
        android:layout_above="@+id/btngocalendar"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="35dp"
        android:text="What is the date?"
        android:textAlignment="center"
        android:textSize="45dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/btngocalendar"
        android:layout_width="266dp"
        android:layout_height="47dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="553dp"
        android:text="Go to calendar"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="102dp" />

</RelativeLayout>